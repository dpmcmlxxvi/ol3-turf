{
  "name": "ol3-turf",
  "version": "0.6.0",
  "description": "A Turf toolbar for OpenLayers.",
  "author": {
    "name": "Daniel Pulido",
    "email": "dpmcmlxxvi@gmail.com"
  },
  "license": "MIT",
  "main": "dist/ol3-turf.js",
  "module": "src/js/main.js",
  "unpkg": "dist/ol3-turf.min.js",
  "jsdelivr": "dist/ol3-turf.min.js",
  "scripts": {
    "build": "run-s bundle",
    "bundle": "run-s bundle:js bundle:css",
    "bundle:css": "run-s bundle:css:*",
    "bundle:css:cat": "shx cat src/css/*.css > dist/ol3-turf.css",
    "bundle:css:min": "cleancss -o dist/ol3-turf.min.css -O2 src/css/*.css",
    "bundle:js": "rollup --silent -c",
    "test": "run-s test:clean test:run test:coverage",
    "test:clean": "shx rm -rf coverage .nyc_output",
    "test:coverage": "nyc report --reporter=lcov",
    "test:run": "jest"
  },
  "homepage": "https://github.com/dpmcmlxxvi/ol3-turf",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpmcmlxxvi/ol3-turf.git"
  },
  "bugs": {
    "url": "https://github.com/dpmcmlxxvi/ol3-turf/issues"
  },
  "keywords": [
    "turf",
    "openlayers",
    "toolbar"
  ],
  "devDependencies": {
    "@turf/turf": "^4.3.1",
    "clean-css-cli": "^4.3.0",
    "coveralls": "^3.0.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^2.0.0",
    "grunt-contrib-cssmin": "^2.2.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^3.0.1",
    "grunt-gh-pages": "^2.0.0",
    "grunt-git": "^1.0.9",
    "grunt-htmlhint": "^0.9.13",
    "grunt-jsdoc": "^2.2.1",
    "grunt-release": "^0.13.1",
    "ink-docstrap": "^1.3.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "jquery": "~3.3.1",
    "jsdoc": "^3.5.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "openlayers": "^4.6.5",
    "puppeteer": "^1.17.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "shx": "^0.3.2"
  },
  "nyc": {
    "include": [
      "**/ol3-turf.js"
    ]
  }
}
